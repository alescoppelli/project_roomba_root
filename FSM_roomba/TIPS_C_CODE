----------------------------------------------------------------
CREARE UN OGGETTO PIN 

mp_obj_t args[] = {
  MP_OBJ_NEW_SMALL_INT(gpio_num),
  other Pin arguments
};
my_pin = machine_pin_type.make_new(&machine_pin_type, n_args, 0, &args);

